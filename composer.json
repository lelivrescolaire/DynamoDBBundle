    "description": "Communicate with a DynamoDB instance from inside your symfony 2 application.",
    "autoload": {
        "psr-0": {
            "LLS\\Bundle\\DynamoDBBundle": ""
        }
    },
    "target-dir": "LLS/Bundle/DynamoDBBundle",
    "require": {
        "php": ">=5.3.0",
        "lelivrescolaire/aws-bundle": "dev-master"
